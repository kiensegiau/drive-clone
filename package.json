{
  "name": "drive-clone",
  "version": "1.0.0",
  "main": "src/index.js",
  "bin": "src/index.js",
  "pkg": {
    "assets": [
      "src/**/*",
      "node_modules/**/*.{js,json,node,dll,exe}",
      "node_modules/axios/**/*",
      "node_modules/axios/dist/**/*",
      "node_modules/axios/lib/**/*",
      "node_modules/firebase/**/*",
      "node_modules/firebase-admin/**/*",
      "node_modules/googleapis/**/*",
      "node_modules/google-auth-library/**/*",
      "node_modules/ffprobe/**/*",
      "node_modules/ffprobe-static/**/*",
      "node_modules/node-disk-info/**/*",
      "node_modules/windows-drive-letters/**/*",
      "node_modules/pdfkit/**/*",
      "node_modules/protobufjs/**/*",
      "node_modules/crypto/**/*",
      "node_modules/javascript-obfuscator/**/*",
      "node_modules/puppeteer-core/**/*",
      "node_modules/puppeteer/**/*"
    ],
    "scripts": [
      "node_modules/**/*.js",
      "node_modules/axios/dist/axios.min.js"
    ],
    "targets": [
      "node16-win-x64"
    ],
    "outputPath": "dist"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "crypto": "^1.0.1",
    "ffprobe": "^1.1.2",
    "ffprobe-static": "^3.1.0",
    "firebase": "^11.0.2",
    "firebase-admin": "^13.0.1",
    "fluent-ffmpeg": "^2.1.3",
    "google-auth-library": "^8.9.0",
    "googleapis": "^105.0.0",
    "got": "^11.8.6",
    "groupdocs-conversion-cloud": "^24.11.0",
    "javascript-obfuscator": "^4.1.1",
    "mammoth": "^1.8.0",
    "node-disk-info": "^1.3.0",
    "node-fetch": "^2.7.0",
    "pdfkit": "^0.13.0",
    "puppeteer-core": "^21.11.0",
    "readline": "^1.3.0",
    "windows-drive-letters": "^4.0.0"
  },
  "scripts": {
    "start": "node src/index.js",
    "build": "npm run obfuscate && pkg .",
    "dev": "nodemon src/index.js",
    "obfuscate": "javascript-obfuscator src --output dist/src --exclude node_modules"
  }
}
